{
  "name": "@arlecchino/api",
  "version": "1.0.0",
  "main": "dist/main.js",
  "author": "bitomic",
  "license": "MIT",
  "private": true,
  "scripts": {
    "hooks": "husky install .github/husky",
    "build": "rimraf tsconfig.tsbuildinfo dist && yarn tsc --listEmittedFiles --incremental",
    "build:watch": "yarn tsc -w --incremental",
    "lint": "eslint src/ --ext .ts --cache",
    "lint:fix": "eslint src/ --fix --ext .ts --cache",
    "start": "yarn node dist/main.js",
    "postinstall": "yarn tsup && tsc --emitDeclarationOnly --declaration"
  },
  "exports": {
    ".": {
      "import": "./dist/main.mjs",
      "require": "./dist/main.js",
      "types": "./dist/main.d.ts"
    }
  },
  "types": "dist/main.d.ts",
  "packageManager": "yarn@3.7.0",
  "devDependencies": {
    "@bitomic/eslint-config": "^1.3.0",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "discord-api-types": "^0.37.67",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "rimraf": "^5.0.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@sapphire/discord-utilities": "^3.2.1",
    "@sapphire/shapeshift": "^3.9.5",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5"
  }
}
